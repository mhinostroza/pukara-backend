{
  "extends": [
    "airbnb-base",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "sourceType": "module",
    "globalReturn": true
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": 1
  }
}
